<Window x:Class="Dock.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dock"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="450">

    <DockPanel LastChildFill="True">
    
        <Button Content="B" DockPanel.Dock="Right"></Button>
        <Button Content="C" DockPanel.Dock="Bottom"></Button>
        <Button Content="A" DockPanel.Dock="Left"></Button>
        <Button Content="A" DockPanel.Dock="Top"></Button>
        <Button Content="Cats" ></Button>
        
    </DockPanel>
    
</Window>
